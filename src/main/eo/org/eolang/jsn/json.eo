+package org.eolang.jsn
+alias org.eolang.collections.map
+rt jvm org.eolang:eo-json:0.0.0

[m] > json
  m > @

  [key] > found
    m.found > res!
    if. > @
      eq.
        res.length
        0
      *
      * (json (res.at 0))

  [index] > at
    m.at index > @

  [] > as-string
    ^.m > @

  # Now we think, that json-array doesn't exist
  [key value] > with
    (json (m.with key value)) > @

  [text] > parse
    json (map *) > @
