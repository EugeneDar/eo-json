+package org.eolang.jsn
+alias org.eolang.collections.list
+alias org.eolang.txt.text
+rt jvm org.eolang:eo-json:0.0.0

[value] > json-array

  value > @

  [] > as-string
    reduce. > res!
      list
        value
      ""
      [a x]
        joined. > @
          text ""
          * a (x.as-string) ", "
    joined. > @
      text ""
      * "[" res "]"

  [index] > at
    value.at index > @

  [v] > with
    json-array (value.with v) > @
