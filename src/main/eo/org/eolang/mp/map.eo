+package org.eolang.mp
+rt jvm org.eolang:eo-json:0.0.0

[m] > map
  m > @

  [t] > put /map

  [key] > get
    memory > res
    memory > j
    seq > @
      res.write "word"
      j.write 0
      while.
        j.less (m.length)
        [i]
          seq > @
            res.write
              if.
                (m.get j).eq key
                (m.get j).get-value
                res
            j.write (j.add 1)
      res
