+package org.eolang.jsn
+alias org.eolang.hamcrest.assert-that
+alias org.eolang.collections.list
+alias org.eolang.jsn.json-int
+alias org.eolang.jsn.json
+junit

[] > hardcode-json-int-as-string
  json-int 42 > a!
  json-int 43 > b!
  assert-that > @
    * (a.as-string) (b.as-string)
    $.equal-to
      list
        * "42" "43"

[] > json-int-creating
  json.of-int -123 > jo!
  assert-that > @
    jo.as-string
    $.equal-to "-123"
