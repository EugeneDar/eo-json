+package org.eolang.mp
+alias org.eolang.mp.map
+alias org.eolang.mp.pair
+junit

[] > create-map
  map * > mp
  TRUE > @

[] > array-of-arrays
  * (* 1 2) (* 3 4 5) > x
  and. > @
    eq.
      (x.get 0).get 1
      2
    eq.
      (x.get 1).get 2
      5

[] > array-of-different-types
  * 1 "abc" > x
  and. > @
    eq.
      x.get 0
      1
    eq.
      x.get 1
      "abc"

[] > pair-getter
  pair 'a' 1 > x
  and. > @
    eq.
      x.get-key
      'a'
    eq.
      x.get-value
      1

[] > array-of-arrays-of-pairs
  * (* (pair 0 1) (pair 'c' 2)) (* (pair 2 "abc") (pair 'b' 'b') (pair 'a' TRUE)) > x
  and. > @
    eq.
      ((x.get 1).get 0).get-value
      "abc"
    eq.
      ((x.get 0).get 1).get-key
      'c'
