+package org.eolang.mp
+alias org.eolang.mp.pair
+junit

[] > pair-getter
  pair "123" 2 > x
  seq > @
    eq.
      x.get-value
      2

[] > pair-key-get
  pair "123" 1 > x
  eq. > @
    x.get-key
    "123"

[] > pair-works-in-arrays
  pair 1 1 > x
  pair 2 1 > y
  * x y > arr
  eq. > @
    (arr.get 1).get-key
    2
